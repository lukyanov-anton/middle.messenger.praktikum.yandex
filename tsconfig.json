{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "typeRoots": ["node_modules/@types", "typing/app.d.ts"],
    "declaration": false,
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "removeComments": true,
    "noEmitOnError": false
  }
}
